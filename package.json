{
  "name": "vue3-mapbox",
  "version": "2.0.0",
  "type": "module",
  "license": "MIT",
  "description": "A Simple mapboxGL Component for Vue 3 write base on maplibre-gl",
  "author": "Danh Nguyen <danh121097@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "maplibre-gl": "^2.4.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@turf/transform-translate": "^6.5.0",
    "@types/turf": "^3.5.32",
    "@vitejs/plugin-vue": "^4.1.0",
    "path": "^0.12.7",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danh121097/vue-mapbox-gl.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-mapbox.umd.cjs",
  "module": "./dist/vue3-mapbox.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-mapbox.js",
      "require": "./dist/vue3-mapbox.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "dist/vue3-mapbox.d.ts",
  "keywords": [
    "vue3",
    "typescript",
    "maplibre-gl",
    "geojson",
    "turfjs"
  ]
}
